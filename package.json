{
  "name": "ui-custom-component",
  "version": "1.0.0",
  "description": "Small webpack based boilerplate for getting started with Boomi Flow UI components",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack -p --config webpack.production.config.js",
    "build-no-source-map": "webpack -p --config webpack.production.config.js --devtool none",
    "upload": "node upload.js",
    "test-unit": "jest --watch",
    "test-integration": "testcafe-live chrome ./src/__tests__/integration",
    "test-integration-remote": "testcafe-live remote ./src/__tests__/integration --qr-code"
  },
  "author": "Dell Boomi",
  "license": "MIT",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.4",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "awesome-typescript-loader": "5.2.0",
    "clipboardy": "^1.2.3",
    "colors": "^1.3.1",
    "css-loader": "0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "glob-promise": "^3.4.0",
    "inquirer": "^6.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "mime-types": "^2.1.19",
    "ngrock-webpack-plugin": "0.0.2",
    "react-test-renderer": "^16.4.1",
    "request-promise-native": "^1.0.5",
    "source-map-loader": "^0.2.3",
    "style-loader": "0.21.0",
    "testcafe": "^0.20.4",
    "testcafe-browser-provider-browserstack": "^1.5.0",
    "testcafe-live": "^0.1.3",
    "ts-jest": "^23.10.4",
    "tslint": "^5.10.0",
    "tslint-loader": "3.6.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "write-file-webpack-plugin": "4.4.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/*.tsx",
      "!**/node_modules/**",
      "!./src/__test__/**/*.*"
    ],
    "setupFiles": [
      "./mocks/manywho.js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/unit.*",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/mocks/styles.js"
    }
  },
  "flow": {
    "tenantId": "88fb88c6-c72a-451a-b3b3-47bab8c8ec41",
    "id": "b7b2964f-0670-4456-8df7-9af5ad222fa5",
    "versionId": "6ddadcd9-9c78-4549-8d92-1090be7bcd9c",
    "filenames": {
      "js": "custom-component.js",
      "css": "custom-component.css"
    }
  }
}
